-    return vasync.pipeline({
-    funcs: [
-        function _mkdir(_, cb) { mkdirp(RULE_PATH, cb); },
-        function _writeRules(_, cb) {
-            return vasync.forEachParallel({
-                inputs: rules,
-                func: function _writeRule(rule, cb2) {
