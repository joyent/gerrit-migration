-
-
-// --- Globals
-
-
-
-var OPTIONS = [
-    {
-        names: ['dir', 'd'],
-        type: 'string',
-        help: 'directory of input files'
-    },
-    {
-        names: ['file', 'f'],
-        type: 'string',
-        help: 'JSON input file'
