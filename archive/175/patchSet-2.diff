-}
-
-if (opts.hasOwnProperty('param')) {
-    opts.param.forEach(function (p) {
-        var parts = p.split('=', 2);
-        if (parts.length != 2) {
-            usage('Invalid parameter: ' + p);
-        }
-
-        params[parts[0]] = parts[1];
-    });
-}
-
-// console.log(JSON.stringify(params, null, 2));
-
-if (params.hasOwnProperty('wrap')) {
